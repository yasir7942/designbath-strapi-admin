{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "model": {
      "type": "string"
    },
    "details": {
      "type": "text"
    },
    "price": {
      "type": "string"
    },
    "salePrice": {
      "type": "string"
    },
    "useBrandDiscount": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "videoLinks": {
      "type": "component",
      "repeatable": true,
      "component": "layout.video-links"
    },
    "IsFixValueDiscount": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
